<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <Button
        android:id="@+id/initiate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_text_initiate_story" />

    <Button
        android:id="@+id/btn_id_see_ongoing_stories"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_text_onging_list" />

    <Button
        android:id="@+id/btn_completed_story_feed"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_text_complete_story_list" />

    <Button
        android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_text_profile" />

</LinearLayout>